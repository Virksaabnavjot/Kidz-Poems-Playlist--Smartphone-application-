<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="free.trail.com.kidzpoems.ireland.dublin.navsingh.org.uk"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/kidzlogo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="free.trail.com.kidzpoems.ireland.dublin.navsingh.org.uk.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="free.trail.com.kidzpoems.ireland.dublin.navsingh.org.uk.VideoPlayer"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/title_activity_video_player"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name="free.trail.com.kidzpoems.ireland.dublin.navsingh.org.uk.TrailExpired"
            android:label="@string/title_activity_trail_expired" >
        </activity>
    </application>

</manifest>
